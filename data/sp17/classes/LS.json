rintInScheduleOfClasses": true, "description": "This course provides students interested in the arts and/or business with an opportunity to develop an idea for an arts organization and turn it into a functioning, sustainable enterprise. Building on each student's own connection to the arts, the course teaches how to invent an arts organization, define its mission, locate the organization within a community, develop its offerings via products, services, and public programs, and manage the organization's numerous operational features."}, "124": {"title": "Consciousness: Buddhist and Neuroscientific Perspectives", "units": {"value": {"units": 3.0}, "type": "fixed"}, "displayName": "LS 124", "meetings": [{"startTime": "14:00:00", "endTime": "15:29:00", "location": {"description": "Evans 60", "code": "EVAN60"}, "dayAbbrvs": "TuTh", "days": {"Tuesday": true, "Sunday": false, "Friday": false, "Saturday": false, "Thursday": true, "Wednesday": false, "Monday": false}, "instructors": [{"name": "David E Presti", "role": "1-TIC"}, {"name": "Robert H. Sharf", "role": "1-TIC"}]}], "id": 31828, "sections": [{"waitlisted": 5, "primary": false, "type": "DIS", "enrolled": 24, "enrollCapacity": 24, "meetings": [{"startTime": "10:00:00", "endTime": "10:59:00", "location": {"description": "Stanley 179", "code": "STAN179"}, "dayAbbrvs": "Mo", "days": {"Tuesday": false, "Sunday": false, "Friday": false, "Saturday": false, "Thursday": false, "Wednesday": false, "Monday": true}, "instructors": []}], "id": 31892, "number": "104", "status": {"description": "Closed", "code": "C"}, "waitlistCapacity": 6, "printInScheduleOfClasses": true}, {"waitlisted": 6, "primary": false, "type": "DIS", "enrolled": 24, "enrollCapacity": 24, "meetings": [{"startTime": "11:00:00", "endTime": "11:59:00", "location": {"description": "Dwinelle 79", "code": "DWIN79"}, "dayAbbrvs": "Mo", "days": {"Tuesday": false, "Sunday": false, "Friday": false, "Saturday": false, "Thursday": false, "Wednesday": false, "Monday": t
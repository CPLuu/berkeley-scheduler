. Cultures in Time", "sections": [{"type": "LEC", "number": "002", "primary": true, "printInScheduleOfClasses": true, "status": {"description": "Open", "code": "O"}, "waitlistCapacity": 75, "enrollCapacity": 75, "waitlisted": 14, "meetings": [{"days": {"Thursday": true, "Sunday": false, "Saturday": false, "Friday": false, "Wednesday": false, "Monday": false, "Tuesday": true}, "location": {"description": "Etcheverry 3106", "code": "ETCH3106"}, "instructors": [{"name": "Kathleen S Moran", "role": null}], "startTime": "15:30:00", "endTime": "16:59:00", "dayAbbrvs": "TuTh"}], "id": 32144, "enrolled": 63}, {"type": "LEC", "number": "001", "primary": true, "printInScheduleOfClasses": true, "status": {"description": "Open", "code": "O"}, "waitlistCapacity": 50, "enrollCapacity": 50, "waitlisted": 16, "meetings": [{"days": {"Thursday": true, "Sunday": false, "Saturday": false, "Friday": false, "Wednesday": false, "Monday": false, "Tuesday": true}, "location": {"description": "Mulford 240", "code": "MULF240"}, "instructors": [{"name": "Christine  Palmer", "role": null}], "startTime": "11:00:00", "endTime": "12:29:00", "dayAbbrvs": "TuTh"}], "id": 12481, "enrolled": 47}, {"type": "LEC", "number": "003", "primary": true, "printInScheduleOfClasses": true, "status": {"description": "Open", "code": "O"}, "waitlistCapacity": 0, "enrollCapacity": 20, "waitlisted": 0, "meetings": [{"days": {"Thursday": false, "Sunday": false, "Saturday": false, "Friday": false, "Wednesday": false, "Monday": true, "Tuesday": false}, "location": {"description": "Kroeber 115", "code": "KROE115"}, "instructors": [{"name": "Mark  Brilliant", "role": null}], "startTime": "14:00:00", "endTime": "16:59:00", "dayAbbrvs": "Mo"}], "id": 32978, "enrolled": 6}], "meetings": [{"days": {"Thursday": true, "Sunday": false, "Saturday": false, "Friday": false, "Wednesday": false, "Monday": false, "Tuesday": true}, "location": {"description": "Mulford 240", "code": "MULF240"}, "instructors": [{"name": "Christine  Palme
ings": [{"endTime": "15:59:00", "location": {"code": "MINR394", "description": "Minor 394"}, "startTime": "15:00:00", "dayAbbrvs": "We", "days": {"Saturday": false, "Wednesday": true, "Tuesday": false, "Friday": false, "Sunday": false, "Thursday": false, "Monday": false}, "instructors": [{"role": null, "name": "Karsten  Gronert"}]}]}, "265": {"title": "Neural Computation", "units": {"type": "fixed", "value": {"units": 3.0}}, "id": 29584, "sections": [{"enrollCapacity": 75, "printInScheduleOfClasses": true, "enrolled": 75, "primary": true, "status": {"code": "C", "description": "Closed"}, "type": "LEC", "id": 29584, "number": "001", "waitlistCapacity": 50, "waitlisted": 6, "meetings": [{"endTime": "16:59:00", "location": {"code": "DONN155", "description": "Donner Lab 155"}, "startTime": "15:30:00", "dayAbbrvs": "TuTh", "days": {"Saturday": false, "Wednesday": false, "Tuesday": true, "Friday": false, "Sunday": false, "Thursday": true, "Monday": false}, "instructors": [{"role": null, "name": "Bruno Adolphus Olshausen"}]}]}], "description": "This course provides an introduction to the theory of neural computation. The goal is to familiarize students with the major theoretical frameworks and models used in neuroscience and psychology, and to provide hands-on experience in using these models. Topics include neural network models, supervised and unsupervised learning rules, associative memory models, probabilistic/graphical models, and models of neural coding in the brain. ", "displayName": "VISSCI 265", "meetings": [{"endTime": "16:59:00", "location": {"code": "DONN155", "description": "Donner Lab 155"}, "startTime": "15:30:00", "dayAbbrvs": "TuTh", "days": {"Saturday": false, "Wednesday": false, "Tuesday": true, "Friday": false, "Sunday": false, "Thursday": true, "Monday": false}, "instructors": [{"role": null, "name": "Bruno Adolphus Olshausen"}]}]}, "212D": {"title": "Anatomy and Vegetative Physiology of the Eye", "units": {"type": "fixed", "value": {"units": 2.0}}, "i